# Example .env file for Visual Search LTI App
# Copy this file to .env and fill in values, or use deploy.sh to generate automatically

# Database Configuration
DB_USER=
DB_PASS=
DB_NAME=
DB_HOST=mongo
MONGO_VERSION=latest

# LTI Configuration
LTI_KEY=
PORT=3000

# Tool Provider Configuration
TOOL_PROVIDER_URL=
TOOL_PROVIDER_NAME=
TOOL_PROVIDER_LOGO=
TOOL_PROVIDER_DESCRIPTION=
TOOL_PROVIDER_REDIRECT_URIS=
TOOL_PROVIDER_AUTO_ACTIVATE=true

